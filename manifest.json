{
    "name":"Remember It!",
    "description":"An Extension for you to remember tasks when you are browsing",
    "manifest_version":2,
    "version":"1.0",
    "icons":{
        "128": "icon128.png",
        "48": "icon48.png",
        "16": "icon16.png"
    },
    "browser_action": {
        "default_icon": "icon16.png",
        "default_popup": "popup.html"
      },
      "permissions": [
        "tabs",
        "storage"
    ],
    "options_page": "options.html"
}